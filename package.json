{
  "name": "mf-tools",
  "version": "1.0.0",
  "description": "An Electron application with Vue and TypeScript",
  "main": "./out/main/index.js",
  "author": {
    "name": "Faith",
    "email": "gxlove_max@163.com"
  },
  "homepage": "https://electron-vite.org",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint --ext .vue,.js,.jsx,.ts,.tsx ./ --max-warnings 0",
    "lint:fix": "eslint --ext .vue,.js,.jsx,.ts,.tsx ./ --max-warnings 0 --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "prepare": "husky",
    "start": "electron-vite preview",
    "dev": "dotenv electron-vite dev",
    "dev:watch": "dotenv electron-vite dev -- -w",
    "debug": "electron-vite -- --inspect --sourcemap --remote-debugging-port=9222",
    "postinstall": "electron-builder install-app-deps",
    "build": "npm run typecheck && electron-vite build",
    "build:check": "pnpm lint && pnpm test",
    "commit": "git cz",
    "build:web": "npm run build",
    "build:unpack": "dotenv npm run build && electron-builder --dir",
    "build:win": "dotenv npm run build && electron-builder --win --x64 --arm64 --publish never",
    "build:win:x64": "dotenv npm run build && electron-builder --win --x64 --publish never",
    "build:win:arm64": "dotenv npm run build && electron-builder --win --arm64 --publish never",
    "build:mac": "dotenv npm run build && electron-builder --mac --arm64 --x64 --publish never",
    "build:mac:arm64": "dotenv npm run build && electron-builder --mac --arm64 --publish never",
    "build:mac:x64": "dotenv npm run build && electron-builder --mac --x64 --publish never",
    "build:linux": "dotenv npm run build && electron-builder --linux --x64 --arm64 --publish never",
    "build:linux:arm64": "dotenv npm run build && electron-builder --linux --arm64 --publish never",
    "build:linux:x64": "dotenv npm run build && electron-builder --linux --x64 --publish never"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,cjs,mjs,cts,mts,vue}": [
      "prettier --write",
      "eslint --fix --no-warn-ignored"
    ],
    "*.{html,vue,css,sass,less,yml,yaml,json}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.2",
    "@electron-toolkit/utils": "^4.0.0",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/vue-table": "^8.21.3",
    "@vueuse/core": "^14.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "electron-updater": "^6.3.9",
    "lucide-vue-next": "^0.563.0",
    "os-proxy-config": "^1.1.2",
    "reka-ui": "^2.8.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "turndown": "^7.2.2",
    "tw-animate-css": "^1.4.0",
    "unplugin-vue-components": "^31.0.0",
    "vue-input-otp": "^0.3.2",
    "vue-router": "^5.0.2",
    "vue-sonner": "^2.0.9"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^7.3.0",
    "@commitlint/cli": "^20.4.1",
    "@commitlint/config-conventional": "^20.4.1",
    "@electron-toolkit/eslint-config-prettier": "3.0.0",
    "@electron-toolkit/eslint-config-ts": "^3.1.0",
    "@electron-toolkit/tsconfig": "^2.0.0",
    "@intlify/eslint-plugin-vue-i18n": "^4.1.1",
    "@types/electron-localshortcut": "^3.1.3",
    "@types/node": "^22.19.1",
    "@types/qs": "^6.14.0",
    "@vitejs/plugin-vue": "^6.0.2",
    "axios": "^1.13.4",
    "commitizen": "^4.3.1",
    "crypto-js": "^4.2.0",
    "cz-git": "^1.12.0",
    "dotenv-cli": "^11.0.0",
    "electron": "^39.2.6",
    "electron-builder": "^26.0.12",
    "electron-devtools-installer": "^4.0.0",
    "electron-localshortcut": "^3.2.1",
    "electron-store": "^11.0.2",
    "electron-vite": "^5.0.0",
    "electron-window-state": "^5.0.3",
    "emittery": "^1.2.0",
    "es-toolkit": "^1.44.0",
    "eslint": "^9.39.1",
    "eslint-plugin-vue": "^10.6.2",
    "fetch-socks": "^1.3.2",
    "he": "^1.2.0",
    "husky": "^9.1.7",
    "i18next": "^25.8.1",
    "ipaddr.js": "^2.3.0",
    "less": "^4.5.1",
    "less-loader": "^12.3.0",
    "lint-staged": "^16.2.7",
    "macos-release": "^3.4.0",
    "node-forge": "^1.3.3",
    "node-machine-id": "^1.1.12",
    "pinia": "^3.0.4",
    "pinia-plugin-persistedstate": "^4.7.1",
    "pinia-shared-state": "^1.0.1",
    "prettier": "^3.7.4",
    "proxy-agent": "^6.5.0",
    "qs": "^6.14.1",
    "sm-crypto-v2": "^1.15.1",
    "typescript": "^5.9.3",
    "undici": "6.21.2",
    "unplugin-auto-import": "^21.0.0",
    "uuid": "^13.0.0",
    "vite": "^7.2.6",
    "vite-plugin-vue-devtools": "^8.0.6",
    "vue": "^3.5.25",
    "vue-eslint-parser": "^10.2.0",
    "vue-i18n": "^11.2.8",
    "vue-tsc": "^3.1.6",
    "windows-release": "^7.1.0",
    "winston": "^3.19.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "electron",
      "esbuild"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-git"
    }
  },
  "packageManager": "pnpm@10.22.0+sha512.bf049efe995b28f527fd2b41ae0474ce29186f7edcb3bf545087bd61fbbebb2bf75362d1307fda09c2d288e1e499787ac12d4fcb617a974718a6051f2eee741c"
}
